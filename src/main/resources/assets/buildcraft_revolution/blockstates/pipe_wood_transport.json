{
  "multipart": [
    {
      "apply": {
        "model": "buildcraft_revolution:block/pipe"
      }
    },
    {
      "apply": {
        "model": "buildcraft_revolution:block/pipe_side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "buildcraft_revolution:block/pipe_side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "buildcraft_revolution:block/pipe_side",
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "buildcraft_revolution:block/pipe_side",
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "buildcraft_revolution:block/pipe_side",
        "x": 270
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "buildcraft_revolution:block/pipe_side",
        "x": 90
      },
      "when": {
        "down": "true"
      }
    }
  ]
}